<keymap version="1" name="Windows copy Proper Redo copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateFavoritesToolWindow">
    <keyboard-shortcut first-keystroke="alt 7" />
  </action>
  <action id="ActivateStructureToolWindow">
    <keyboard-shortcut first-keystroke="alt 2" />
  </action>
  <action id="ActivateVersionControlToolWindow">
    <keyboard-shortcut first-keystroke="alt 9" />
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt back_space" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="Git.Fetch">
    <keyboard-shortcut first-keystroke="shift alt 8" />
  </action>
  <action id="Git.Pull">
    <keyboard-shortcut first-keystroke="shift alt 7" />
  </action>
  <action id="Maven.Reimport">
    <keyboard-shortcut first-keystroke="shift alt 5" />
  </action>
  <action id="Maven.ReimportProject">
    <keyboard-shortcut first-keystroke="alt 5" />
  </action>
  <action id="NewClass">
    <keyboard-shortcut first-keystroke="shift alt 1" />
  </action>
  <action id="NewDir">
    <keyboard-shortcut first-keystroke="shift alt 3" />
  </action>
  <action id="NewFile">
    <keyboard-shortcut first-keystroke="shift alt 2" />
  </action>
  <action id="NewModuleInGroup">
    <keyboard-shortcut first-keystroke="shift alt 4" />
  </action>
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="shift ctrl alt z" />
  </action>
  <action id="Vcs.ShowTabbedFileHistory">
    <keyboard-shortcut first-keystroke="shift alt 0" />
  </action>
</keymap>